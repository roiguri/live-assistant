{
    "manifest_version": 3,
    "name": "Live Stream AI Assistant",
    "version": "1.0",
    "description": "Real-time AI assistant with screen capture",
    
    "permissions": [
        "storage"
    ],

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
  }